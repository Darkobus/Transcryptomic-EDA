   "source": [
    "class ProgressBar:\n",
    "    def __init__(self, total_epochs):\n",
    "        self.pbar = tqdm(total=total_epochs, desc=\"UMAP Training Progress\", unit=\"epoch\")\n",
    "\n",
    "    def __call__(self, epoch, n_epochs):\n",
    "        self.pbar.n = epoch\n",
    "        self.pbar.refresh()\n",
    "\n",
    "n_epochs = 200\n",
    "progress_bar = ProgressBar(total_epochs=n_epochs)"
   ]
  },
